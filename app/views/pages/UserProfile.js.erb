		function setRandomColour() {
		  var letters = '0123456789ABCDEF';
		  var color = '#';
		  for (i = 0; i < 6; i++) {
		    color += letters[Math.floor(Math.random() * 16)];
		  }
		  console.log(color);
		  return color;
		}
		function search(start,vehicle,array){
			var newPos=0;
			
			for (k = start; k < array.length; k++) {
				
		    	if(vehicle===array[k]){
		    		newPos=k;
		    		return newPos;
		    	}
		  	}
		  	return newPos;
		}

